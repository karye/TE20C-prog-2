<Window x:Class="Labb2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Labb2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="5">
        <Label >Slumpa fram ett tall 1-100</Label>
        <Button Click="ClickSlumpa">Slumpa!</Button>
        <TextBox Name="rutaResultat" FontSize="50" TextAlignment="Center"></TextBox>
    </StackPanel>
</Window>
